<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="public" extends="abstract_struts" namespace="/">				
		<action name="welcome" class="action.Welcome">
			<result>/jsp/town/welcome.jsp</result>
		</action>
		
		
		<action name="AIOBack" class="action.AioBack">
			<result>null</result>
		</action>
		
		<action name="AIORemote" class="action.AioRemote">
			<result>null</result>
		</action>
		
		<action name="AIOReceiver" class="action.AIOReceiver">
			<result>null</result>
		</action>
		
		<action name="simView" class="action.SimView">
			<result>/jsp/front.jsp</result>
		</action>
		
		<action name="into" class="action.Into">
			<result>/jsp/Main.jsp</result>
		</action>
		
		
		
		<action name="login" class="action.Login">
			<result>/jsp/manager/orderManager.jsp</result>
		</action>
		
		
		
		<action name="finish" class="action.Finish">
			<result>/jsp/finish.jsp</result>
			<result name="redirect" type="redirect">${url}</result>
		</action>
		
		<action name="manager" class="action.Manager">
			<result>/jsp/manager/manager.jsp</result>
		</action>
		
		<action name="productManager" class="action.ProductManager">
			<result>/jsp/manager/productManager.jsp</result>
		</action>
		
		<action name="orderManager" class="action.OrderManager">
			<result>/jsp/manager/orderManager.jsp</result>
		</action>
		
		<action name="checkEmail" class="action.CheckEmail">
			<result>/jsp/checkEmail.jsp</result>
		</action>
		
		<action name="bodyManager" class="action.BodyManager">
			<result>/jsp/manager/bodyManager.jsp</result>
		</action>
		
	</package>
	
	
	
	

</struts>